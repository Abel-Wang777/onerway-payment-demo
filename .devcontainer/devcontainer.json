{
    "name": "Onerway Payment Demo",
    "image": "mcr.microsoft.com/devcontainers/java:17",
    "postCreateCommand": "mvn clean install -DskipTests",
    "customizations": {
        "vscode": {
            "extensions": [
                "vscjava.vscode-java-pack",
                "redhat.vscode-yaml"
            ],
            "settings": {
                "java.server.launchMode": "Standard"
            }
        }
    },
    "forwardPorts": [
        8080
    ]
}